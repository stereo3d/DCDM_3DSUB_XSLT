      <Subtitle SpotNumber="1" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:01:02" TimeOut="00:00:04:02">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">We are not the first civilization</Text>
      </Subtitle>
      <Subtitle SpotNumber="2" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:04:04" TimeOut="00:00:05:12">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">dedicating our knowledge,</Text>
      </Subtitle>
      <Subtitle SpotNumber="3" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:05:21" TimeOut="00:00:07:13">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">skills and resources in an</Text>
      </Subtitle>
      <Subtitle SpotNumber="4" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:07:15" TimeOut="00:00:08:17">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">effort to understand</Text>
      </Subtitle>
      <Subtitle SpotNumber="5" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:08:19" TimeOut="00:00:10:02">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">these cosmic origins.</Text>
      </Subtitle>
      <Subtitle SpotNumber="6" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:10:12" TimeOut="00:00:12:21">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">As early as two thousand years ago, the</Text>
      </Subtitle>
      <Subtitle SpotNumber="7" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:12:23" TimeOut="00:00:15:03">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Maya people followed the same quest.</Text>
      </Subtitle>
      <Subtitle SpotNumber="8" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:21:04" TimeOut="00:00:22:17">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">They built cities in the middle of an</Text>
      </Subtitle>
      <Subtitle SpotNumber="9" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:22:19" TimeOut="00:00:23:20">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">impenetrable jungle.</Text>
      </Subtitle>
      <Subtitle SpotNumber="10" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:24:17" TimeOut="00:00:26:06">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Temples, palaces and</Text>
      </Subtitle>
      <Subtitle SpotNumber="11" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:26:08" TimeOut="00:00:27:21">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">pyramids that served as</Text>
      </Subtitle>
      <Subtitle SpotNumber="12" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:27:23" TimeOut="00:00:30:01">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Astronomic Observation Stations.</Text>
      </Subtitle>
      <Subtitle SpotNumber="13" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:30:17" TimeOut="00:00:32:14">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Some of the cities held as many as sixty</Text>
      </Subtitle>
      <Subtitle SpotNumber="14" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:32:16" TimeOut="00:00:35:10">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">thousand people, all this to</Text>
      </Subtitle>
      <Subtitle SpotNumber="15" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:35:12" TimeOut="00:00:37:04">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">make their vision come true.</Text>
      </Subtitle>
      <Subtitle SpotNumber="16" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:42:10" TimeOut="00:00:45:05">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">The Plasamayor, dominated by the Jaguar</Text>
      </Subtitle>
      <Subtitle SpotNumber="17" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:45:07" TimeOut="00:00:46:05">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Temple towering</Text>
      </Subtitle>
      <Subtitle SpotNumber="18" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:46:07" TimeOut="00:00:48:10">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">forty-seven meters above the square.</Text>
      </Subtitle>
      <Subtitle SpotNumber="19" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:48:22" TimeOut="00:00:50:16">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Originally the walls were covered with</Text>
      </Subtitle>
      <Subtitle SpotNumber="20" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:50:18" TimeOut="00:00:53:12">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">plaster and painted vivid blue and red.</Text>
      </Subtitle>
      <Subtitle SpotNumber="21" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:54:11" TimeOut="00:00:56:20">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">But the solar solstice, the light falling</Text>
      </Subtitle>
      <Subtitle SpotNumber="22" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:56:22" TimeOut="00:00:58:07">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">through this gate, lands</Text>
      </Subtitle>
      <Subtitle SpotNumber="23" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:58:09" TimeOut="00:00:59:19">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">on the opposite pyramid.</Text>
      </Subtitle>
      <Subtitle SpotNumber="24" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:08:17" TimeOut="00:01:11:12">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">For seventeen hundred years, Tikal played</Text>
      </Subtitle>
      <Subtitle SpotNumber="25" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:11:14" TimeOut="00:01:12:23">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">a central role as the</Text>
      </Subtitle>
      <Subtitle SpotNumber="26" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:13:01" TimeOut="00:01:14:12">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">largest and most important</Text>
      </Subtitle>
      <Subtitle SpotNumber="27" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:14:14" TimeOut="00:01:16:02">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">city of the Mayan Kingdom.</Text>
      </Subtitle>
      <Subtitle SpotNumber="28" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:16:12" TimeOut="00:01:18:14">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">The collective knowledge of architecture,</Text>
      </Subtitle>
      <Subtitle SpotNumber="29" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:18:21" TimeOut="00:01:20:07">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">astronomy and medicine</Text>
      </Subtitle>
      <Subtitle SpotNumber="30" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:20:09" TimeOut="00:01:22:04">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">must have been staggering.</Text>
      </Subtitle>
      <Subtitle SpotNumber="31" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:24:11" TimeOut="00:01:36:16">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">But none of it has been put in writing</Text>
      </Subtitle>
      <Subtitle SpotNumber="32" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:36:18" TimeOut="00:01:39:05">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">and scientists can only speculate.</Text>
      </Subtitle>
      <Subtitle SpotNumber="33" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:40:13" TimeOut="00:01:41:15">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Knowledge was handed down</Text>
      </Subtitle>
      <Subtitle SpotNumber="34" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:41:17" TimeOut="00:01:43:01">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">verbally over the millennia.</Text>
      </Subtitle>
      <Subtitle SpotNumber="35" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:44:06" TimeOut="00:01:45:12">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">From priest to priest,</Text>
      </Subtitle>
      <Subtitle SpotNumber="36" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:46:05" TimeOut="00:01:47:13">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">from master to apprentice.</Text>
      </Subtitle>
      <Subtitle SpotNumber="37" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:48:11" TimeOut="00:01:50:16">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">But the real treasure of the Maya lies</Text>
      </Subtitle>
      <Subtitle SpotNumber="38" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:50:18" TimeOut="00:01:52:04">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">directly beneath us.</Text>
      </Subtitle>
      <Subtitle SpotNumber="39" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:29:00" TimeOut="00:02:31:18">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">The Maya call him Himman, the knowing,</Text>
      </Subtitle>
      <Subtitle SpotNumber="40" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:32:15" TimeOut="00:02:35:10">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">priest and healer in one person who still</Text>
      </Subtitle>
      <Subtitle SpotNumber="41" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:35:12" TimeOut="00:02:36:21">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">has the ancient knowledge.</Text>
      </Subtitle>
      <Subtitle SpotNumber="42" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:47:04" TimeOut="00:02:48:21">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">The Herman lives at the shores of Lake</Text>
      </Subtitle>
      <Subtitle SpotNumber="43" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:48:23" TimeOut="00:02:50:12">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Petenitsa, where there</Text>
      </Subtitle>
      <Subtitle SpotNumber="44" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:50:14" TimeOut="00:02:52:09">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">are only some 20 people left to</Text>
      </Subtitle>
      <Subtitle SpotNumber="45" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:52:11" TimeOut="00:02:53:11">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">understand and speak</Text>
      </Subtitle>
      <Subtitle SpotNumber="46" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:53:13" TimeOut="00:02:54:10">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">his native tongue.</Text>
      </Subtitle>
      <Subtitle SpotNumber="47" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:54:23" TimeOut="00:02:56:22">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">If it disappears, so will</Text>
      </Subtitle>
      <Subtitle SpotNumber="48" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:57:00" TimeOut="00:02:58:10">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">the treasure of the Mayans.</Text>
      </Subtitle>
      <Subtitle SpotNumber="49" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:59:01" TimeOut="00:03:01:10">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">For you cannot write down what he knows,</Text>
      </Subtitle>
      <Subtitle SpotNumber="50" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:01:20" TimeOut="00:03:02:16">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">since it has never</Text>
      </Subtitle>
      <Subtitle SpotNumber="51" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:02:18" TimeOut="00:03:04:01">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">been written down before.</Text>
      </Subtitle>
      <Subtitle SpotNumber="52" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:46:06" TimeOut="00:03:47:12">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">He is still familiar with</Text>
      </Subtitle>
      <Subtitle SpotNumber="53" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:47:14" TimeOut="00:03:49:05">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">the ancient healing practices,</Text>
      </Subtitle>
      <Subtitle SpotNumber="54" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:49:18" TimeOut="00:03:50:16">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">with the herbs and</Text>
      </Subtitle>
      <Subtitle SpotNumber="55" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:50:18" TimeOut="00:03:51:23">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">potions that have brought relief</Text>
      </Subtitle>
      <Subtitle SpotNumber="56" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:52:01" TimeOut="00:03:53:00">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">to people suffering</Text>
      </Subtitle>
      <Subtitle SpotNumber="57" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:53:02" TimeOut="00:03:54:12">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">from all kinds of ailments</Text>
      </Subtitle>
      <Subtitle SpotNumber="58" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:54:17" TimeOut="00:03:55:12">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">for centuries.</Text>
      </Subtitle>
      <Subtitle SpotNumber="59" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:56:00" TimeOut="00:03:57:13">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">And he is just as familiar</Text>
      </Subtitle>
      <Subtitle SpotNumber="60" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:57:15" TimeOut="00:03:59:21">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">with the Popol Vuh, the Mayan book</Text>
      </Subtitle>
      <Subtitle SpotNumber="61" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:59:23" TimeOut="00:04:01:06">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">of God-given rites,</Text>
      </Subtitle>
      <Subtitle SpotNumber="62" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:04:01:14" TimeOut="00:04:03:09">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">according to which the body echoes</Text>
      </Subtitle>
      <Subtitle SpotNumber="63" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:04:03:11" TimeOut="00:04:05:12">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">the patient's spiritual imbalance.</Text>
      </Subtitle>
      <Subtitle SpotNumber="64" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:04:05:23" TimeOut="00:04:08:18">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">To restore balance, he initiates a ritual</Text>
      </Subtitle>
      <Subtitle SpotNumber="65" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:04:08:20" TimeOut="00:04:10:05">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">to enable the patient to</Text>
      </Subtitle>
      <Subtitle SpotNumber="66" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:04:10:07" TimeOut="00:04:12:01">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">make contact with his ancestors,</Text>
      </Subtitle>
      <Subtitle SpotNumber="67" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:04:12:14" TimeOut="00:04:14:01">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">because who knows better</Text>
      </Subtitle>
      <Subtitle SpotNumber="68" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:04:14:03" TimeOut="00:04:16:05">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">what puts strain on the spirit</Text>
      </Subtitle>
      <Subtitle SpotNumber="69" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:04:16:16" TimeOut="00:04:19:04">
        <Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">than the ones who are now spirits?</Text>
      </Subtitle>
